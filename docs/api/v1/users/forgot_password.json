{
  "published": true,
  "name": "Forgot password",
  "end_point": "api/v1/users/forgot_password",
  "http_method": "GET",
  "params": [
    {
    "key": "email",
    "required": true,
    "type": "string",
    "description": "email for account check"
    }
  ],
  "header": {
    "Accept": "application/vnd.auction_backend.v1",
    "Content-Type": "application/json"
  },
  "examples": [{
    "request": {
      "email":"abc@gmail.com"
    },
    "response": {
      "message": "An Email to reset password is sent to your email.",
      "status": 200
    }
  }]
}
