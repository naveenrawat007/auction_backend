{
  "published": true,
  "name": "New password",
  "end_point": "api/v1/users/password/new",
  "http_method": "POST",
  "params": [
    {
      "key": "user[password]",
      "required": true,
      "type": "string",
      "description": "password to set"
    }
  ],
  "header": {
    "Authorization": "Token",
    "Accept": "application/vnd.auction_backend.v1",
    "Content-Type": "application/json"
  },
  "examples": [{
    "request": {
      "user":{
    		"password": "123456"
    	}
    },
    "response": {
      "message": "Password changed sucessfully. Now you can login.",
      "status": 200
    }
  }]
}
